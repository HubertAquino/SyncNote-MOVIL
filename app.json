  "expo": {
    "name": "SyncNote",
    "slug": "syncnote",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "syncnote",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
